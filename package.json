{
    "name": "piranha-extension",
    "displayName": "Piranha Extension",
    "publisher": "EktoromSoftwareInc",
    "description": "Syntax highlighting support for Piranha.",
    "icon": "images/PiranhaLogo_v01.png",
    "version": "0.1.1",
    "engines": {
        "vscode": "^1.34.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
      "type": "git",
      "url": "https://github.com/ange-yaghi/piranha-vscode-extension.git"
    },
    "contributes": {
        "languages": [
            {
                "id": "piranha",
                "aliases": [
                    "Piranha"
                ],
                "extensions": [
                    ".mr"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "piranha",
                "scopeName": "source.mr",
                "path": "./syntaxes/piranha.tmLanguage.json"
            }
        ]
    }
}

{
    "name": "piranha-extension",
    "displayName": "Piranha Extension",
    "description": "Syntax highlighting support for Piranha.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.34.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "piranha",
                "aliases": [
                    "Piranha"
                ],
                "extensions": [
                    ".mr"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "piranha",
                "scopeName": "source.mr",
                "path": "./syntaxes/piranha.tmLanguage.json"
            }
        ]
    }
}